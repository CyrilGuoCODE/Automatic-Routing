{
  "name": "automatic-routing",
  "version": "1.0.0",
  "description": "Automatic-Routing是一个智能域名路由系统，可以根据用户的地理位置、网络延迟和DNS解析时间，自动将用户重定向到最佳的域名访问路径。该系统帮助网站所有者优化用户的访问体验，减少加载时间，提高服务可靠性。",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyrilGuoCODE/Automatic-Routing.git"
  },
  "keywords": [
    "routing",
    "dns",
    "performance",
    "geolocation"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyrilGuoCODE/Automatic-Routing/issues"
  },
  "homepage": "https://github.com/CyrilGuoCODE/Automatic-Routing#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
